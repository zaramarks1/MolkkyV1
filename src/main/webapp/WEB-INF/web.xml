<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" version="4.0">
  <servlet>
    <description></description>
    <display-name>Teams</display-name>
    <servlet-name>Teams</servlet-name>
    <servlet-class>com.servlet.Teams</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Teams</servlet-name>
    <url-pattern>/</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>Main</display-name>
    <servlet-name>Main</servlet-name>
    <servlet-class>com.servlet.Main</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Main</servlet-name>
    <url-pattern>/main</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>TeamsWithCSV</display-name>
    <servlet-name>TeamsWithCSV</servlet-name>
    <servlet-class>com.servlet.TeamsWithCSV</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>TeamsWithCSV</servlet-name>
    <url-pattern>/teamsWithCSV</url-pattern>
  </servlet-mapping>
  
    <jsp-config>
        <jsp-property-group>
            <url-pattern>*.jsp</url-pattern>
            <include-prelude>/WEB-INF/taglibs.jsp</include-prelude>
        </jsp-property-group>
    </jsp-config>
    
</web-app>